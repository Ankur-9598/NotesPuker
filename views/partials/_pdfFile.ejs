<div class="card category__file">
    <h3><%=pdf.title%></h3>
    <div class="category__fileBottom">
      <div class="d-flex">
        <p class="upvoteBtn" onclick="updateUpvote('<%= pdf.user %>', '<%= pdf.title%>', '<%= pdf._id %>')"><a id="liker" class="profileUrl">
            <i class="mdi mdi-apple-keyboard-caps"></i>
            <span>Upvote</span>
            <span id="<%= pdf._id %>"><%=pdf.upvotes.length%></span>
          </a></p>
        <p>
          <a class="profileUrl" href="#">
            <i class="mdi mdi-download"></i>
            <span>Download</span>
          </a>
        </p>
        <p>
          <a class="profileUrl" href="/profile/<%=pdf.user%>">
            <i class="mdi mdi-account"></i>
            <span><%=pdf.userName%></span>
          </a>
        </p>
      </div>
      <div class="category__fileBottomDate">
        <p>Posted on <%=pdf.createdAt%></p>
      </div>
    </div>
  </div>